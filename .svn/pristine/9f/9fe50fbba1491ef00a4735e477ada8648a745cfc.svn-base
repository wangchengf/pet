<script type="text/javascript" src="./resource/script/musicPlay-2.0.min.js"></script>
<script>
    var musicBox = document.querySelector('.music_box'), musicAudio;
    musicBox && (musicAudio = new MusicPlay('.music_box'));
    $(function(){
        window.setTimeout(function(){
            createbjMusic("");
        },3000);
    });
    function createbjMusic(e) {
        $(".music_box").show();
        $("#music").attr("src", e);
        if( undefined == window.localStorage || (window.localStorage && ! window.localStorage.getItem('audio_pause')) ){
            isPaused = false;
            musicAudio.play();
            _musicPlay();
        }else{
            isPaused = true;
            musicAudio.pause();
            _musicPaused(); // 关闭
        }
    }

</script>

