<script>$.ajaxSetup({cache:true,beforeSend:function(a,d){var b=$("meta[http-equiv='X-LOGIN-ID']").eq(0)[0];var c=$("meta[http-equiv='X-LOGIN-TOKEN']").eq(0)[0];if(c&&c.content&&b&&b.content){a.setRequestHeader("X-LOGIN-ID",b.content);a.setRequestHeader("X-LOGIN-TOKEN",c.content)}},complete:function(b,a){if(a=="error"){c(b,b.status)}function c(e,f){switch(f){case 401:location.href="/wap.php?act=auth&do=loginIndex&backUrl="+encodeURIComponent(location.href);break;case 404:alert("接口不存在");break;case 500:alert("服务器内部错误");break;case 503:alert("缺少请求参数:"+(e.responseJSON.msg?e.responseJSON.msg:""));break;case 409:break;case 301:case 302:var d=e.getResponseHeader("Location");if(!d){d=e.getResponseHeader("location")}if("string"==typeof d){d=d.replace(" ","");if(d.indexOf("?")!=-1){location.href=d+"&backUrl="+encodeURIComponent(location.href)}else{location.href=d+"?backUrl="+encodeURIComponent(location.href)}}break}}}});</script>


